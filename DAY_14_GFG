class Solution {
  public:
    int search(vector<int>& arr, int key) {
    // Loop through each element in the array
    for(int i = 0; i < arr.size(); i++) {
        // If key is found, return the index
        if(arr[i] == key) {
            return i;
        }
    }
    // If the loop completes and the key is not found, return -1
    return -1;
}

};
